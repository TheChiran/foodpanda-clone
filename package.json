    "axios": "^1.3.4",
    "babel-jest": "^29.5.0",
    "framer-motion": "^10.10.0",
    "leaflet": "^1.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.10.0",
    "sass": "^1.60.0",
    "swiper": "^9.3.0"
  },
